<product xmlns:o="http://test/" o:name="Product Template">
	<status/>
	<title/>
	<description/>
	<shortCode/>
	<startTime o:nature="time"/>
	<endTime o:nature="time"/>
	<limit o:nature="measurement"/>
	<features o:nature="collection" o:memberNature="object">
		<feature>
			<name/>
		</feature>
	</features>
	<limits o:nature="collection" o:memberNature="object">
		<limit>
			<name/>
			<limit o:nature="measurement"/>
		</limit>
	</limits>
	<options o:nature="collection" o:memberNature="object">
		<option>
			<name/>
			<pricingBasis o:nature="link" o:assocType="basis"/>
		</option>
	</options>
	<usesAccountTemplate o:nature="link" o:assocType="template"/>
</product>
